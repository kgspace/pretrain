{
    "root_dir": "../",
    "log_dir": "logs/",
    "result_dir": "results/",
    "process_dir": "processes/",
    "checkpoint_dir": "checkpoints/",
    "dataset_dir": "datasets/",
    "run_dir": "runs/",
    "device": "gpu",
    "seed": 2020,
    "validate": 1,
    "metrics": ["ndcg_at_k", "precision_at_k", "recall_at_k", "map_at_k"],
    "validate_metric": "ndcg_at_k@10",
    "valid_k": 10,
    "result_file": "lightgcn_result.csv",
    "dataset": "ml_100k",
    "data_split": "leave_one_out",
    "model": "lightgcn",
    "config_id": "default",
    "keep_pro": 0.6,
    "batch_size": 1000,
    "regs": [1e-5],
    "emb_dim": 64,
    "layer_size": [64, 64, 64],
    "lr": 1e-3,
    "optimizer": "adam",
    "max_epoch": 100,
    "save_name": "lightgcn.model"
}
