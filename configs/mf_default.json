{
    "model": "MF",
    "config_id": "default",
    "root_dir": "../",
    "dataset": "tafeng",
    "data_split": "leave_one_out",
    "validate": 1,
    "emb_dim": 64,
    "num_negative": 4,
    "batch_size": 500,
    "dropout": 0.0,
    "metrics": ["ndcg_at_k", "precision_at_k", "recall_at_k", "map_at_k"],
    "validate_metric": "ndcg_at_k@10",
    "device": "gpu",
    "optimizer": "adam",
    "loss": "bce",
    "lr": 1e-3,
    "reg": 1e-3,
    "max_epoch": 100,
    "result_file": "mf_result.csv",
    "log_dir": "logs/",
    "result_dir": "results/",
    "checkpoint_dir": "checkpoints/",
    "dataset_dir": "datasets/",
    "run_dir": "runs/",
    "save_name": "mf.model"
}
