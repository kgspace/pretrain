{
    "model": "compgcn_mf",
    "config_id": "default",
    "root_dir": "../",
    "pre_train": 0,
    "dataset": "ml_100k",
    "data_split": "leave_one_out",
    "validate": 1,
    "emb_dim": 64,
    "base_dim": 64,
    "late_dim": 64,
    "n_bases": 10,
    "num_negative": 4,
    "batch_size": 500,
    "amp": true,
    "metrics": [
        "ndcg_at_k",
        "precision_at_k",
        "recall_at_k",
        "map_at_k"
    ],
    "validate_metric": "ndcg_at_k@10",
    "device": "gpu",
    "optimizer": "adam",
    "loss": "bce",
    "lr": 1e-3,
    "reg": 1e-3,
    "max_epoch": 200,
    "result_file": "compgcn_mf_result.csv",
    "log_dir": "logs/",
    "result_dir": "results/",
    "checkpoint_dir": "checkpoints/",
    "dataset_dir": "datasets/",
    "run_dir": "runs/",
    "save_name": "compgcn_mf.model",
    "process_dir": "processes/",
    "activator": "tanh",
    "dropout": 0.0,
    "opn": "sub",
    "compgcn_config": {
        "save_name": "compgcn.model"
    }
}