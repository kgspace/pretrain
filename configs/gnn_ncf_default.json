{
    "model": "ncf_gcn",
    "model_comments": "'gcn', 'mlp', 'ncf', 'ncf_gcn'",
    "config_id": "default",
    "root_dir": "../",
    "dataset": "ml_100k",
    "pre_train": 0,
    "data_split": "leave_one_out",
    "validate": 1,
    "emb_dim": 64,
    "num_negative": 4,
    "batch_size": 1000,
    "dropout": 0.0,
    "metrics": ["ndcg_at_k", "precision_at_k", "recall_at_k", "map_at_k"],
    "validate_metric": "ndcg_at_k@10",
    "device": "gpu",
    "optimizer": "adam",
    "activator": "lrelu",
    "lr": 1e-3,
    "max_epoch": 10,
    "result_file": "ncf_gcn_result.csv",
    "log_dir": "logs/",
    "result_dir": "results/",
    "checkpoint_dir": "checkpoints/",
    "dataset_dir": "datasets/",
    "run_dir": "runs/",
    "gcn_config": {
        "name": "gcn",
        "dropout": 0.0,
        "layers": [64, 64, 64],
        "regs": 1e-5,
        "save_name": "gcn.model"
    },
    "mlp_config": {"name": "mlp", "n_layers": 3, "save_name": "mlp.model"},
    "neumf_config": {"name": "ncf", "save_name": "ncf.model"}
}
